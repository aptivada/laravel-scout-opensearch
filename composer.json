{
    "name": "soeurngsar/laravel-scout-opensearch",
    "description": "Laravel scout driver for OpenSearch",
    "authors": [
        {
            "name": "Sar Soeurng",
            "email": "sar.soeurng@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "laravel/framework": "^11.0",
        "roave/better-reflection": "^6.14.0",
        "laravel/scout": "^8.0|^9.0|^10.0",
        "opensearch-project/opensearch-php": "^2.0",
        "illuminate/support": "^9.0|^10.0",
        "shyim/opensearch-php-dsl": "^1.0"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "SoeurngSar\\LaravelScoutOpenSearch\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SoeurngSar\\LaravelScoutOpenSearch\\Providers\\OpenSearchServiceProvider",
                "SoeurngSar\\LaravelScoutOpenSearch\\Providers\\ScoutOpenSearchServiceProvider"
            ]
        }
    }
}
